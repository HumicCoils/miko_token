{
  "name": "miko-keeper-bot",
  "version": "1.0.0",
  "description": "MIKO Token Keeper Bot - Automated fee management and distribution",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [
    "solana",
    "keeper",
    "bot",
    "miko"
  ],
  "author": "MIKO Labs",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.91.0",
    "@solana/spl-token": "^0.4.0",
    "@coral-xyz/anchor": "^0.30.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.7",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "winston": "^3.11.0",
    "twitter-api-v2": "^1.16.0",
    "@iarna/toml": "^2.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/node-cron": "^3.0.11",
    "@types/jest": "^29.5.11",
    "@types/iarna__toml": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}