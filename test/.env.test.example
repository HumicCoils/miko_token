# TEST CONFIGURATION FOR DEVNET
# Copy this file to .env.test and fill in your values

# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com

# Program IDs (replace with your devnet deployments)
ABSOLUTE_VAULT_PROGRAM=YOUR_ABSOLUTE_VAULT_PROGRAM_ID
SMART_DIAL_PROGRAM=YOUR_SMART_DIAL_PROGRAM_ID

# Token Configuration
MIKO_TOKEN_MINT=YOUR_DEVNET_MIKO_TOKEN_MINT

# Wallets
TREASURY_WALLET=YOUR_TREASURY_WALLET_PUBKEY
OWNER_WALLET=YOUR_OWNER_WALLET_PUBKEY

# Keeper Bot
KEEPER_BOT_PRIVATE_KEY=YOUR_TEST_WALLET_PRIVATE_KEY_BASE64

# Test Settings (DO NOT MODIFY)
MIN_HOLDER_BALANCE=100000000000000  # 100,000 MIKO (9 decimals)
SKIP_PRICE_CHECKS=true
SKIP_SWAPS=true
USE_MOCK_TWITTER=true
RUN_IMMEDIATELY=true  # Run AI monitor immediately for testing

# External APIs (not used in test mode)
TWITTER_BEARER_TOKEN=NOT_USED_IN_TEST_MODE
BIRDEYE_API_KEY=NOT_USED_IN_TEST_MODE

# Monitoring
HEALTH_CHECK_PORT=3000
LOG_LEVEL=debug